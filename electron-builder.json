{
  "productName": "Bypass Cors",
  "asar": true,
  "appId": "com.github.chrishham.BypassCors",
  "extraFiles": [
    {
      "from": "build/images",
      "to": "resources",
      "filter": [
        "**/*"
      ]
    }
  ],
  "publish": {
    "provider": "github",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage"
    }
  }
}